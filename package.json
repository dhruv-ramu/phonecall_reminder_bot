{
  "name": "discord-reminder-bot",
  "version": "1.0.0",
  "description": "Discord bot that schedules reminders and triggers Twilio voice calls",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "bot",
    "reminder",
    "twilio",
    "voice",
    "call",
    "scheduler"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bullmq": "^5.1.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "ioredis": "^5.3.2",
    "moment": "^2.29.4",
    "twilio": "^4.20.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/discord-reminder-bot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/discord-reminder-bot/issues"
  },
  "homepage": "https://github.com/yourusername/discord-reminder-bot#readme"
}
